.factionPanel {
    width: 200px;
    height: 200px;
    background-size: cover;
    background-position: center;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-left-radius: 0;
    border-bottom-right-radius: 25px;
    padding: 0;
    border-top-width: 0;
    border-left-width: 0;
    overflow: hidden;
}

.factionImage {
    width: 100%;
    height: 100%;
    overflow: hidden;
}